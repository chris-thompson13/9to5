<h1>Edit this boats information</h1>
<%= form_with model: @boat do |f| %>
<%= hidden_field_tag :user_id %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :location %>
<%= f.text_field :location %>
<br>
<%= f.label :containers %>
<%= f.text_field :amount %>
<br>
<%= f.file_field :avatar %>
<%= f.submit %>
<% end %>
